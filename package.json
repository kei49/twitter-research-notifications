{
  "name": "@kei49/twitter-research-notifications",
  "version": "0.0.1",
  "description": "Twitter Research Notifications",
  "author": "@kei49",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:kei49/twitter-research-notifications.git"
  },
  "bugs": {
    "url": "https://github.com/kei49/twitter-research-notifications/issues"
  },
  "scripts": {
    "start": "nodemon --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "pm2": "./node_modules/.bin/pm2 start ./src/index.ts --name twitter-research-notifications",
    "pm2:restart": "./node_modules/.bin/pm2 restart twitter-research-notifications",
    "lint": "tslint './src/**/*.{ts}'",
    "prettier": "prettier --write 'src/**/*.ts*'",
    "prettier-check": "prettier --list-different 'src/**/*.ts*'"
  },
  "engines": {
    "node": "16.2.0"
  },
  "engineStrict": true,
  "dependencies": {
    "@slack/webhook": "^6.1.0",
