    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "node-cron": "^3.0.0",
    "node-localstorage": "^2.2.1",
    "nodemon": "^2.0.15",
    "pm2": "^5.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.19",
    "@types/node-cron": "^3.0.1",
    "@types/node-localstorage": "^1.3.0",
    "prettier": "2.5.1",
    "sequelize-cli": "^6.4.1"
  }
